      <default>'pcmanfm'</default>
      <summary>Default</summary>
      <description>Default</description>
    </key>
    <key type="s" name="exec">
      <default>'pcmanfm'</default>
      <summary>Default</summary>
      <description>Default</description>
    </key>
    <key type="b" name="needs-term">
      <default>false</default>
      <summary>needs terminal</summary>
      <description>Whether the application needs a terminal to run.</description>
    </key>
  </schema>
  <schema path="/desktop/noname/applications/archiver/" id="desktop.noname.applications.archiver">
    <key type="s" name="default">
      <default>'file-roller'</default>
      <summary>Default</summary>
      <description>Default</description>
    </key>
    <child schema="desktop.noname.applications.archiver.file-roller" name="file-roller"/>
  </schema>

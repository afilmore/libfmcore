      <default>'lxtask'</default>
      <summary>Default</summary>
      <description>Default</description>
    </key>
    <key type="s" name="exec">
      <default>'lxtask'</default>
      <summary>Default</summary>
      <description>Default</description>
    </key>
  </schema>
  <schema path="/desktop/noname/applications/panel/" id="desktop.noname.applications.panel">
